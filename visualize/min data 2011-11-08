Pytanie o daty 
SELECT count(*) from ngrams_small WHERE data > '2011-11-18' AND data < '2011-12-01'


SELECT klub_id, data from ngrams_small WHERE data = '2011-11-19'AND ngram LIKE 'o';


# SQL Join do zdobycia tych ngramów

SELECT klub_id, data from ngrams AS a 
JOIN ngram_dictionary AS b ON 
    b.id = a.ngram_id 

WHERE   data > '2011-11-18' AND 
        data < '2012-05-01' AND 
        b.ngram LIKE 'kultury';




